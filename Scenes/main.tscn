script/source = "extends Control

var id = self

func _ready():
	get_tree().paused = true
	self.rect_global_position.y = -self.rect_size.y
	global.connect('game_over', id, 'game_over')

func _on_tween_tween_completed(object, key): 
	get_tree().paused = true
	pass

func game_over():
	$tween.interpolate_property(self, 'rect_global_position:y', self.rect_global_position.y, 0, 1.0, Tween.TRANS_BACK, Tween.EASE_OUT)
	$tween.start()
	
func _on_button_pressed():
	global.score = 0
	get_tree().reload_current_scene()
"

